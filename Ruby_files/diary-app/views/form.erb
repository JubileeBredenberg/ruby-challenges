<!-- Form ERB Template for Cheer app -->
<div class="row">

<div class="col-md-2">
<p>You win some<br>You win some later</p>
</div>

<% if !@error.nil? %>
    <p><%= @error %></p>
<% end %>

<form class="col-md-3" action="/" method="post">
